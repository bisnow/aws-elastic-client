{
    "name": "renoki-co/aws-elastic-client",
    "description": "Elastic Client handler for Laravel that supports AWS Elasticsearch service.",
    "keywords": ["laravel", "php", "elastic", "elasticsearch", "client", "aws"],
    "license": "Apache-2.0",
    "homepage": "https://github.com/renoki-co/aws-elastic-client",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "aws/aws-sdk-php": "^3.132",
        "babenkoivan/elastic-client": "^1.2",
        "guzzlehttp/guzzle": "^6.5|7.0",
        "guzzlehttp/ring": "^1.1.1",
        "illuminate/support": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\AwsElasticHandler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\AwsElasticHandler\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.1",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^5.0|^6.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "RenokiCo\\AwsElasticHandler\\AwsElasticHandlerServiceProvider"
            ]
        }
    }
}
